<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.home.ResultDashboardFragment">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/lblResult"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Final Results"
        android:textColor="@color/colorText"
        android:textSize="40sp"
        android:fontFamily="@font/merienda_regular"
        android:textAlignment="center"
        android:drawableTop="@drawable/oneimage"
        android:drawableStart="@drawable/twoimage"
        android:drawableEnd="@drawable/threeimage"/>

    <!--<Spinner
        android:id="@+id/spinnerCollege"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/mySpinnerItemStyleWhite"
        android:layout_below="@+id/lblResult"
        android:layout_marginTop="10dp"/>-->

    <FrameLayout
        android:id="@+id/flGetClg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/lblResult" />

    <Spinner
        android:id="@+id/spinnerEvents"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/mySpinnerItemStyleWhite"
        android:layout_below="@+id/flGetClg"
        android:layout_marginTop="10dp"/>

</RelativeLayout>