<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nilay.android.eventhook">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".home.EventHookApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SampleActivity"></activity>
        <activity
            android:name=".volunteer.VolunteerActivity"
            android:label="@string/title_activity_volunteer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".coordinator.CoordinatorActivity"
            android:label="@string/title_activity_coordinator"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".students.StudentActivity"
            android:label="@string/title_activity_student"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".collegeadmin.ClgAdminActivity"
            android:label="@string/title_activity_clg_admin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mainadmin.AdminActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_admin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".registration.RegistrationActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".home.HomeTwoActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".home.HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>